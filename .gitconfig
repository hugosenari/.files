[color]
	diff = auto
	status = auto
	branch = auto
[branch]
	autosetuprebase = always
[alias]
	b = branch
	bl = branch --all
	bd = branch -D
	bs = branch --no-merged
	bname = rev-parse --abbrev-ref HEAD
	cd = checkout
	cdb = checkout -b
	master = checkout master
	head = checkout head
	c = commit -m
	d = diff --word-diff
	g = pull
	get = pull
	m = merge
	mc = merge --continue
	md = merge dev
	mm = merge master
	mt = mergetool
	p = push
	post = push
	r = rebase master
	rc = rebase --continue
	s = status -s
	undo = reset --hard
	ls = ls-tree --name-only
	lsm = ls-tree --name-only master
[push]
	default = tracking
